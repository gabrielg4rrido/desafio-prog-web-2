generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id        String   @id @db.VarChar(32)
  userId    String   @db.VarChar(32)
  items     Json
  total     Float    @db.DoublePrecision
  status    String   @db.VarChar(32)
  createdAt DateTime @default(now())
}
